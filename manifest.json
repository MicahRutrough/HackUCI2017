{
	"manifest_version": 2,
	"name": "MoodGraffr",
	"description": "Track your mood over time, and in relation to external factors!",
	"version": "1.0",
	"icons": { "16": "icon-16.png",
          "128": "icon-128.png" },
	"manifest_version": 2,
	"options_page": "window.html",
	"browser_action":
	{
		"default_icon": "icon-16.png"
	},
	"background":
	{
		"scripts": ["background.js"],
		"persistent": true
	},
	"permissions":
	[
	"activeTab",
	"tabs",
	"alarms",
	"debugger",
	"https://ajax.googleapis.com/",
	"geolocation",
	"http://api.openweathermap.org/",
	"http://maps.google.com/",
	"storage"
	]
}

