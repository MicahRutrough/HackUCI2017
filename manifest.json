{
	"manifest_version": 2,
	"name": "Hello, World!",
	"description": "This is a test chrome app for HackUci",
	"version": "0.1",
	"manifest_version": 2,
	"browser_action":
	{
		"default_icon": "icon-16.png"
	},
	"background":
	{
		"scripts": ["background.js"],
		"persistent": true
	},
	"permissions":
	[
	"activeTab",
	"tabs",
	"alarms",
	"debugger",
	"https://ajax.googleapis.com/",
	"geolocation",
	"http://api.openweathermap.org/",
	"http://maps.google.com/",
	"storage"
	]
}

